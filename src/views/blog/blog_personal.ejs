<section class="py-5">
    <div class="container px-5 my-5">
        <div class="row gx-5">
            
            <div class="col-lg-9">
                <table class="table">
                    <thead>
                      <tr>
                        <th scope="col"></th>
                        <th scope="col">#</th>
                        <th scope="col">Title</th>
                        <th scope="col">Create At</th>
                        <th scope="col">Update At</th>
                        <th scope="col">Action</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% blogs.forEach((blog,index) => { %>
                        <tr>
                          <th scope="row" ><input type="checkbox" class="form-check-input" id="exampleCheck1"></th>
                          <th scope="row"><%= index + 1 %></th>
                          <td><%= blog.title %></td>
                          <td> <%= new Date(blog.createdAt).toLocaleDateString('vi-VN') %></td>
                          <td> <%= new Date(blog.updatedAt).toLocaleDateString('vi-VN') %></td>
                          <td> <span><i class="bi bi-pencil-square"></i></span> <span><i class="bi bi-x-lg"></i></span> </td>
                          
                        </tr>  
                      <% }) %>
                  
                    </tbody>
                  </table>
            </div>
            <div class="col-lg-3">

            </div>
        </div>
    </div>
</section>